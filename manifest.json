{
  "name": "Mark This !!",
  "description": "This extension lets you mark different sections on the pages you visited and consolidate them.",
  "version": "1.0.0",
  "background": {
    "scripts": ["background.js"]
  },
  "icons": { "128": "paw_128x128.png" },
  "permissions": [
    "contextMenus",
    "tabs", "http://*/*", "https://*/*"
  ],
  "browser_action": {
    "default_title": "Show selected marks.",
    "default_icon": "paw_16x16.png"
  },
  "manifest_version": 2
}
